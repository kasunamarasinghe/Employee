@model Employee.Models.Emplyee

@{
    ViewBag.Title = "EmployeeDetails";
}

<h2>Employee Details</h2>
<table style="font-family:'Times New Roman'">
    <tr>
    <td>Employee ID</td>
    <td>@Model.EID</td>
    </tr>
      <tr>
    <td>Employee Name</td>
    <td>@Model.EmployeeName</td>
    </tr>
      <tr>
    <td>Employee Gender</td>
    <td>@Model.Gender</td>
    </tr>
      <tr>
    <td>Employee City</td>
    <td>@Model.City</td>
    </tr>
</table>